{
  "projectSetup": {
    "projectName": "test-health-api",
    "description": "Test health record system with new field types",
    "author": "Test User",
    "license": "MIT",
    "nodeVersion": "20",
    "packageManager": "npm"
  },
  "databaseConfig": {
    "databaseType": "MongoDB",
    "provider": "Atlas",
    "connectionString": "mongodb+srv://user:pass@cluster.mongodb.net/test",
    "autoMigration": "push"
  },
  "modelDefinition": {
    "models": [
      {
        "name": "Worker",
        "timestamps": true,
        "fields": [
          {
            "name": "workerId",
            "type": "string",
            "required": true,
            "unique": true,
            "indexed": true
          },
          {
            "name": "fullName",
            "type": "string",
            "required": true
          },
          {
            "name": "dateOfBirth",
            "type": "date"
          },
          {
            "name": "gender",
            "type": "enum",
            "values": ["male", "female", "other"]
          },
          {
            "name": "phoneNumber",
            "type": "string",
            "indexed": true
          },
          {
            "name": "bloodGroup",
            "type": "string"
          },
          {
            "name": "sensitiveEncrypted",
            "type": "json"
          },
          {
            "name": "tags",
            "type": "string[]"
          }
        ],
        "relationships": [
          {
            "type": "one-to-many",
            "targetModel": "Visit",
            "fieldName": "visits"
          }
        ]
      },
      {
        "name": "Visit",
        "timestamps": true,
        "fields": [
          {
            "name": "visitDate",
            "type": "datetime",
            "default": "now"
          },
          {
            "name": "reason",
            "type": "string"
          },
          {
            "name": "diagnoses",
            "type": "string[]"
          },
          {
            "name": "prescriptions",
            "type": "json[]"
          }
        ],
        "relationships": [
          {
            "type": "many-to-one",
            "targetModel": "Worker",
            "fieldName": "workerId"
          }
        ]
      }
    ]
  },
  "authConfig": {
    "enabled": true,
    "method": "jwt",
    "jwt": {
      "accessTTL": "15m",
      "refreshTTL": "7d",
      "rotation": true,
      "blacklist": true
    },
    "roles": ["admin", "doctor", "worker"]
  },
  "featureSelection": {
    "cors": true,
    "helmet": true,
    "compression": true,
    "validation": true,
    "logging": true,
    "caching": false,
    "swagger": true,
    "health": true,
    "rateLimit": true,
    "versioning": false
  }
}
