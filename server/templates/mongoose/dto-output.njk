import { ApiProperty } from '@nestjs/swagger';

export class {{ model.outputDtoName }} {
  @ApiProperty({
    description: 'Unique identifier',
    example: '507f1f77bcf86cd799439011',
  })
  id!: string;

{% for field in model.fields %}
  {# Exclude sensitive fields from output DTOs #}
  {% if field.name !== 'password' and field.name !== 'refreshToken' and field.name !== 'passwordResetToken' and field.name !== 'emailVerificationToken' %}
  {% if field.apiExample %}
  @ApiProperty({
    description: '{{ field.apiDescription | default(field.name) }}',
    example: {{ field.apiExample | safe }},
    {% if field.type === 'array' %}type: 'array',{% endif %}
  })
  {% endif %}
  {{ field.name }}!: {{ field.tsType }};

  {% endif %}
{% endfor %}
{% if model.timestamps %}
  @ApiProperty({
    description: 'Creation timestamp',
    example: '2024-01-01T00:00:00.000Z',
  })
  createdAt!: Date;

  @ApiProperty({
    description: 'Last update timestamp',
    example: '2024-01-01T00:00:00.000Z',
  })
  updatedAt!: Date;
{% endif %}
}
